{
  "name": "tbf-special-offers",
  "version": "1.0.0",
  "description": "TBF Special Offers widget",
  "main": "index.js",
  "author": "Kurmaeva Daria",
  "dependencies": {},
  "devDependencies": {
    "aws-sdk": "^2.189.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.15.0",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mime": "^2.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!**/__mocks__/**",
      "!**/__test__/**",
      "!**/__snapshots__/**",
      "!**/constants.js",
      "!**/index.css"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.node/",
      "/jest/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 80,
        "lines": 80,
        "statements": -30
      }
    }
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "test": "jest",
    "deploy": "node ./scripts/deploy.js"
  }
}
